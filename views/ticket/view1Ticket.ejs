<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View Ticket</title>
    <!-- AdminLTE CSS -->
    <link rel="stylesheet" href="/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="/dist/css/adminlte.min.css">
    <style>
        .container {
            max-width: 600px;
            margin-top: 30px;
        }
        .bg-light {
            padding: 10px;
            border-radius: 5px;
        }
    </style>
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  <!-- Navbar -->
  <nav class="main-header navbar navbar-expand navbar-white navbar-light">
    <!-- Left navbar links -->
    <ul class="navbar-nav">
      <li class="nav-item">
        <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
      </li>
      <li class="nav-item d-none d-sm-inline-block">
        <a href="/home1" class="nav-link">Home</a>
      </li>
      <li class="nav-item d-none d-sm-inline-block">
        <a href="#" class="nav-link">Contact</a>
      </li>
    </ul>
    <!-- Right navbar links -->
    <ul class="navbar-nav ml-auto">
      <!-- Navbar Search -->
      <li class="nav-item">
        <a class="nav-link" data-widget="navbar-search" href="#" role="button">
          <i class="fas fa-search"></i>
        </a>
        <div class="navbar-search-block">
          <form class="form-inline">
            <div class="input-group input-group-sm">
              <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
              <div class="input-group-append">
                <button class="btn btn-navbar" type="submit">
                  <i class="fas fa-search"></i>
                </button>
                <button class="btn btn-navbar" type="button" data-widget="navbar-search">
                  <i class="fas fa-times"></i>
                </button>
              </div>
            </div>
          </form>
        </div>
      </li>
    </ul>
  </nav>

  <!-- Main Sidebar Container -->
  <aside class="main-sidebar sidebar-dark-primary elevation-4">
    <!-- Sidebar -->
    <div class="sidebar">
      <!-- Sidebar Menu -->
      <nav class="mt-2">
        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
          <li class="nav-item">
            <a href="#" class="nav-link">
              <i class="nav-icon fas fa-table"></i>
              <p>
                Tables  
                <i class="fas fa-angle-left right"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a href="/tickets" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Tickets List</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/clients" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>clients table</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/demandeurs" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>demandeur table </p>
                </a>
              </li>
             

              <li class="nav-item">
                <a href="/utilisateurs" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>users table</p>
                </a>
              </li>
            </ul>
          </li>

          <li class="nav-item">
            <a href="/utilisateurs/userRole" class="nav-link">
              <i class="far fa-circle nav-icon"></i>
              <p>Users role</p>
            </a>
          </li>
              <!-- Add other table links here -->
            </ul>
          </li>
        </ul>
      </nav>
      <!-- /.sidebar-menu -->
    </div>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-primary">Ticket Details</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/home1">Home</a></li>
              <li class="breadcrumb-item"><a href="/tickets">Tickets</a></li>
              <li class="breadcrumb-item"><a href="/tickets/en-attente">Tickets en attente</a></li>
              <li class="breadcrumb-item"><a href="/tickets/resolu">Tickets resolu</a></li>
              <li class="breadcrumb-item"><a href="/tickets/rejete">Tickets rejete</a></li>
              <li class="breadcrumb-item"><a href="/tickets/necessiteIntervention">Tickets besoin intervention</a></li>
              <li class="breadcrumb-item"><a href="/tickets/en-cours">Tickets en cours</a></li>


              <li class="breadcrumb-item active">Ticket Details</li>
            </ol>
          </div>
        </div>
      </div>
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container">
        <div class="bg-white p-4 shadow-sm rounded">
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Description:</label>
                <div class="p-2 bg-light rounded"><%= ticket.description %></div>
            </div>
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Date Reception:</label>
                <div class="p-2 bg-light rounded"><%= new Date(ticket.datereception).toLocaleString() %></div>
            </div>
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Notes:</label>
                <div class="p-2 bg-light rounded"><%= ticket.notes %></div>
            </div>
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Client:</label>
                <div class="p-2 bg-light rounded"><%= ticket.client_nom %></div>
            </div>
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Demandeur:</label>
                <div class="p-2 bg-light rounded"><%= ticket.demandeur_nom %></div>
            </div>
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Etat:</label>
                <div class="p-2 bg-light rounded"><%= ticket.etat %></div>
            </div>
            <div class="ticket-info mb-3">
                <label class="font-weight-bold text-secondary">Priorite:</label>
                <div class="p-2 bg-light rounded"><%= ticket.priorite %></div>
            </div>
           
        </div>
      </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <footer class="main-footer">
    <strong>&copy; 2024 <a href="https://adminlte.io">AdminLTE.io</a>.</strong>
    All rights reserved.
    <div class="float-right d-none d-sm-inline-block">
      <b>Version</b> 3.2.0
    </div>
  </footer>

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
  </aside>
</div>
<!-- ./wrapper -->

<!-- jQuery -->
<script src="/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap 4 -->
<script src="/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- AdminLTE App -->
<script src="/dist/js/adminlte.min.js"></script>

</body>
</html>
